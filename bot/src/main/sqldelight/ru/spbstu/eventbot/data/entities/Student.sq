CREATE TABLE Student (
id INTEGER PRIMARY KEY AUTOINCREMENT,
chat_id INTEGER NOT NULL UNIQUE,
email TEXT NOT NULL,
name TEXT NOT NULL,
group_number TEXT NOT NULL
);

selectAll:
SELECT *
FROM Student;

findByChatId:
SELECT *
FROM Student
WHERE chat_id = ?;

insert:
INSERT OR REPLACE INTO Student
VALUES (NULL, :chatId, :email, :name, :group);
